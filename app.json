{
  "expo": {
    "name": "DeptZap",
    "slug": "deptzap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barnagoz.deptzap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.barnagoz.deptzap"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0a074dd5-19d6-4087-9910-b9f0280cc18c"
      }
    },
    "owner": "e5vos.technikusi.szervezet"
  }
}
